<%@ Master Language="C#" AutoEventWireup="true" Inherits="WebSite.MP" CodeBehind="~/Fr/MP.Master.cs" %>
	<!DOCTYPE html>
	<html lang="fr">

	<head runat="server">
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<link rel="stylesheet" href="https://cdn.comelin.com/cdn/bootstrap4-large-grid.min.css">
		<link href="/css/comelin2018.min.css" rel="stylesheet" type="text/css" runat="server" id="linkCSS" />
		<meta name="theme-color" content="#DF660B">
		<asp:ContentPlaceHolder ID="head" runat="server">
		</asp:ContentPlaceHolder>

		<!-- Google Tag Manager -->
